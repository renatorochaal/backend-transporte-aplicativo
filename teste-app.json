            "body": {
              "mode": "raw",
              "raw": "{\n    \"cpf_pass_viag\": \"12345678901\",\n    \"cpf_mot_viag\": \"12345678901\",\n    \"placa_veic_viag\": \"ABC1234\",\n    \"local_orig_viag\": \"Rua A, 123\",\n    \"local_dest_viag\": \"Rua B, 456\",\n    \"dt_hora_inicio\": \"2023-05-23T15:00:00Z\",\n    \"dt_hora_fim\": \"2023-05-23T16:00:00Z\",\n    \"qtde_pass\": 3,\n    \"forma_pagto\": \"DINHEIRO\",\n    \"valor_pagto\": 100.50,\n    \"cancelam_mot\": \"N\",\n    \"cancelam_pass\": \"N\"\n}"
            },
            "url": {
              "raw": "{{BASE_URL}}/viagens",
              "host": ["{{BASE_URL}}"],
              "path": ["viagens"]
            }
          }
        },
