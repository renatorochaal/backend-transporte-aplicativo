      "item": [
        {
          "name": "GET Viagens por Marca",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{BASE_URL}}/viagens/marca?marca={{marca}}&data={{data}}&horaInicio={{horaInicio}}&horaFim={{horaFim}}",
              "host": ["{{BASE_URL}}"],
              "path": ["viagens", "marca"],
              "query": [
                { "key": "marca", "value": "{{marca}}" },
                { "key": "data", "value": "{{data}}" },
                { "key": "horaInicio", "value": "{{horaInicio}}" },
                { "key": "horaFim", "value": "{{horaFim}}" }
              ]
            }
          }
        },
